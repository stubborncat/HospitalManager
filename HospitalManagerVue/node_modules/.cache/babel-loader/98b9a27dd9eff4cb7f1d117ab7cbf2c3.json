estDrug();\n    this.requestCheck();\n  },\n};\n</script>\n<style lang=\"scss\">\n\n// 让文字居中\n.drugRigth{\n  text-align: center;\n}\n.el-tag{\n  margin: 8px;\n}\n.rigthTable{\n  margin-top: 56px;\n  margin-left: 8px;\n}\n.drugInput{\n  margin-top: 8px;\n  margin-bottom: 8px;\n}\n.el-row{\n    margin: 5px;\n}\n.orderInput{\n    width: 240px;\n}\n.el-pagination{\n  margin: 8px;\n}\n</style>"],"mappings":"AAwKA,OAAAA,SAAA;AACA,SAAAC,QAAA;AACA,OAAAC,OAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,GAAA;MACAC,GAAA;MACAC,GAAA;MACAC,KAAA;MACAC,OAAA;MACAC,MAAA;MACAC,KAAA;MACAC,QAAA;MACAC,IAAA;MACAC,UAAA;MACAC,KAAA;MACAC,SAAA;MACAC,WAAA;MACAC,cAAA;MACA;MACAC,MAAA;MACA;MACAC,SAAA;MACAC,UAAA;MACAC,UAAA;MACAC,SAAA;MACAC,eAAA;MACAC,eAAA;MACAC,YAAA;MACA;MACAC,iBAAA;IAEA;EACA;EACAC,OAAA;IACA;IACAC,iBAAAC,IAAA,EAAAC,UAAA;MACA7B,OAAA,CAAA8B,GAAA;QACAC,MAAA;UACAH,IAAA,EAAAA,IAAA;UACAC,UAAA,EAAAA;QACA;MACA,GACAG,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAA/B,IAAA,CAAAgC,MAAA;UACA,KAAAC,QAAA,CAAAC,KAAA;QACA;MACA;IACA;IACA;IACAC,YAAA;MAEA,SAAAC,CAAA,MAAAA,CAAA,QAAAvB,WAAA,CAAAwB,MAAA,EAAAD,CAAA;QACA,KAAAX,gBAAA,MAAAZ,WAAA,CAAAuB,CAAA,EAAAV,IAAA,OAAAb,WAAA,CAAAuB,CAAA,EAAAE,KAAA;MACA;MAAA;MACA,IAAAtC,IAAA;QACAC,GAAA,OAAAsC,WAAA,GAAAtC,GAAA;QACA;QACA;QACAuC,OAAA,OAAAD,WAAA,GAAAC,OAAA;QACAC,KAAA,OAAAF,WAAA,GAAAE,KAAA;QACAC,MAAA,OAAAH,WAAA,GAAAG,MAAA;QACAC,WAAA,OAAAJ,WAAA,GAAAI;MAAA;MACA7C,OAAA,CAAA8C,IAAA,2BAAA5C,IAAA,EACA8B,IAAA,CAAAC,GAAA;QACAc,OAAA,CAAAC,GAAA,CAAAf,GAAA;QACA,IAAAA,GAAA,CAAA/B,IAAA,CAAAgC,MAAA;UACA,KAAAC,QAAA,CAAAC,KAAA;UACA;QACA;;QAEA;QACApC,OAAA,CAAA8B,GAAA;UACAC,MAAA;YACA5B,GAAA,EAAAD,IAAA,CAAAC;UACA;QACA,GACA6B,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA,CAAA/B,IAAA,CAAAgC,MAAA,UACA,KAAAC,QAAA,CAAAc,OAAA;QAEA;QAIA,KAAAC,OAAA,CAAAC,IAAA;QACAJ,OAAA,CAAAC,GAAA,CAAAf,GAAA,CAAA/B,IAAA;MACA,GACAkD,KAAA,CAAAC,GAAA;QACAN,OAAA,CAAAX,KAAA,CAAAiB,GAAA;MACA;IAEA;IACA;IACAZ,YAAA;MACA,IAAAE,KAAA;MACA,IAAAC,MAAA;MACA,IAAAC,WAAA;MACA,IAAA1C,GAAA,QAAAA,GAAA;MACA,IAAAuC,OAAA,QAAAzB,MAAA;MACA,SAAAqB,CAAA,MAAAA,